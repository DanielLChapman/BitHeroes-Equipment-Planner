@require './_*.styl'

body {
  width: 100%;
  height: 100%;
  margin: 0;
  padding: 0;
}
.App {
  text-align: center;
}

.App-logo {
  animation: App-logo-spin infinite 20s linear;
  height: 80px;
}

.App-header {
  background-color: #222;
  height: 150px;
  padding: 20px;
  color: white;
}

.App-title {
  font-size: 1.5em;
}

.App-intro {
  font-size: large;
}

.by-set-reveal {
  position: relative;
  left: 00px;
}
.by-set-reveal-sub {
  position: relative;
  right: -50px;
}

.by-slot-types span {
  font-size: 16px;
}